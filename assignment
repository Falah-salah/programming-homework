import java.util.*;
	class HomeWork{
		public static void main(String args[]){
			System.out.println("nawakat chan pita");
				Scanner x=new Scanner(System.in);
					int y=x.nextInt();
						String name[]=new String[y];

			for(int r=0;r<name.length;r++){
				System.out.println("enter letter number "+(r+1)+":");
					Scanner i=new Scanner(System.in);
						name [r]=i.next();
}
			for (int s=0;s<y;s++){
				System.out.print(name[s]);



}
			System.out.println();












	}
	}
